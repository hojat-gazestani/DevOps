[0m[32mâ”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€[0m

]1337;SetMark\[1m[34mâ”„â”„â”„[0m[1m[34mAutostarting KVM instances[0m

[32mâ”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€[0m
[33m# Enable the VM autostart: 
[0m[33mvirsh autostart kvm1
[0m[33m
[0m[33m# Obtain information for the instance: 
[0m[33mvirsh dominfo kvm1
[0m[33m
[0m[33m# Stop the running instance and ensure that it is in the shut off state: 
[0m[33mvirsh destroy kvm1
[0m[33mvirsh list --all
[0m[33m
[0m[33m# Stop the libvirt daemon and ensure that it is not running: 
[0m[33m/etc/init.d/libvirt-bin stop
[0m[33mpgrep -lfa libvirtd
[0m[33m
[0m[33m# Start back the libvirt daemon:
[0m[33m/etc/init.d/libvirt-bin start
[0m[33m
[0m[33m# List all running instances: 
[0m[33mvirsh list --all
[0m[33m
[0m[33m# Disable the autostart option: 
[0m[33mvirsh autostart kvm1 --disable
[0m[33m
[0m[33m# Verify the change: 
[0m[33mvirsh dominfo kvm1 | grep -i autostart
[0m[33m 
[0m[32mâ”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€[0m

]1337;SetMark\[1m[34mâ”„â”„â”„[0m[1m[34mWorking with storage pools[0m

[32mâ”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€[0m
[33m# Copy the raw Debian image file 
[0m[33mcp /tmp/kvm1.img /var/lib/libvirt/images/
[0m[33m
[0m[33m# Create the following storage pool definition: 
[0m[33mcat file_storage_pool.xml
[0m[33m<pool type="dir">
[0m[33m  <name>file_virtimages</name>
[0m[33m  <target>
[0m[33m    <path>/var/lib/libvirt/images</path>
[0m[33m  </target>
[0m[33m</pool>
[0m[33m
[0m[33m# Cannot access storage file, Permission denied Error in KVM Libvirt
[0m[33msudo vim /etc/libvirt/qemu.conf
[0m[33m	user = "hoji"
[0m[33m	group = "libvirt"
[0m[33m
[0m[33msudo systemctl restart libvirtd
[0m[33msudo usermod -a -G libvirt $(whoami)
[0m[33msudo chown hoji:libvirt /var/lib/libvirt/images
[0m[33m
[0m[33m# Define the new storage pool
[0m[33mvirsh pool-define file_storage_pool.xml
[0m[33m
[0m[33m# List all storage pools: 
[0m[33mvirsh pool-list --all
[0m[33m
[0m[33m# Start the new storage pool and ensure that it's active: 
[0m[33mvirsh pool-start file_virtimages
[0m[33mvirsh pool-list --all
[0m[33m
[0m[33m# Enable the autostart feature on the storage pool: 
[0m[33mvirsh pool-autostart file_virtimages
[0m[33mvirsh pool-list --all
[0m[33m 
[0m[33m# Obtain more information about the storage pool: 
[0m[33mvirsh pool-info file_virtimages
[0m[33m
[0m[33m# List all volumes that are a part of the storage pool: 
[0m[33mvirsh vol-list file_virtimages
[0m[33m
[0m[33m# Obtain information on the volume:
[0m[33mvirsh vol-info /var/lib/libvirt/images/kvm1.img
[0m[33m
[0m[33m# Start new KVM instance using the storage pool and volume
[0m[33mvirt-install --name kvm1 --ram 1024 --graphics vnc,listen=146.20.141.158 --hvm --disk vol=file_virtimages/kvm1.img --import
[0m[33m
[0m[33mvirsh list --all
[0m[32mâ”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€[0m

]1337;SetMark\[1m[34mâ”„â”„â”„[0m[1m[34mManaging volumes[0m

[32mâ”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€[0m
[33m# List the available storage pools: 
[0m[33mvirsh pool-list --all
[0m[33m
[0m[33m# List the available volumes, that are a part of the storage pool:
[0m[33mvirsh vol-list file_virtimages
[0m[33m
[0m[33m# Create a new volume with the specified size:
[0m[33mvirsh vol-create-as file_virtimages new_volume.img 9G
[0m[33m
[0m[33m# List the volumes on the filesystem: 
[0m[33mls -lah /var/lib/libvirt/images/
[0m[33m 
[0m[33m# Obtain information about the new volume: 
[0m[33mqemu-img info /var/lib/libvirt/images/new_volume.img
[0m[33m 
[0m[33m# Use the virsh command to get even more information:
[0m[33mvirsh vol-info new_volume.img --pool file_virtimages
[0m[33m
[0m[33m# Dump the volume configuration: 
[0m[33mvirsh vol-dumpxml new_volume.img --pool file_virtimages
[0m[33m 
[0m[33m# Resize the volume and display the new size: 
[0m[33mvirsh vol-resize new_volume.img 10G --pool file_virtimages
[0m[33mvirsh vol-info new_volume.img --pool file_virtimages
[0m[33m
[0m[33m# Delete the volume and list all available volumes in the storage pool: 
[0m[33mvirsh vol-delete new_volume.img --pool file_virtimages
[0m[33mvirsh vol-list file_virtimages
[0m[33m
[0m[33m# Clone the existing volume: 
[0m[33mvirsh vol-clone kvm1.img kvm2.img --pool file_virtimages
[0m[33mvirsh vol-list file_virtimages
[0m[33m
