[0m[36m			<alias name='serial0'/>
[0m[36m		</console>
[0m[36m		<input type='mouse' bus='ps2'/>
[0m[36m		<input type='keyboard' bus='ps2'/>
[0m[36m		<graphics type='vnc' port='5900' autoport='yes'
[0m[36mlisten='0.0.0.0'>
[0m[36m			<listen type='address' address='0.0.0.0'/>
[0m[36m		</graphics>
[0m[36m		<video>
[0m[36m			<model type='cirrus' vram='16384' heads='1'/>
[0m[36m			<alias name='video0'/>
[0m[36m			<address type='pci' domain='0x0000' bus='0x00' slot='0x02'
[0m[36mfunction='0x0'/>
[0m[36m		</video>
[0m[36m		<memballoon model='virtio'>
[0m[36m			<alias name='balloon0'/>
[0m[36m			<address type='pci' domain='0x0000' bus='0x00' slot='0x04'
[0m[36mfunction='0x0'/>
[0m[36m		</memballoon>
[0m[36m	</devices>
[0m[36m</domain>
[0m"""

conn = libvirt.[33mopen[0m('[36mqemu:///system[0m')
[32mif[0m conn == [33mNone[0m:
	[32mprint[0m('[36mFailed to connecto to the hypervizor[0m')
	[33mexit[0m([95m1[0m)

instance = conn.[33mdefineXML[0m(xmlconfig)
[32mif[0m instance == [33mNone[0m:
	[32mprint[0m('[36mFailed to define the instance[0m')
	[33mexit[0m([95m1[0m)

instances = conn.[33mlistDefinedDomains[0m()
[32mprint[0m('[36mDefined instances: [0m[91m{[0m[91m}[0m'.[33mformat[0m(instances))

conn.[33mclose[0m()
