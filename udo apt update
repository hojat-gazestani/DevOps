]1337;SetMark\[1m[34mâ”„â”„â”„â”„[0m[1m[34mCreate Ubuntu 22.04 KVM Guest From Cloud Image[0m

[32mâ”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€[0m
[33msudo mkdir /var/lib/libvirt/images/templates
[0m[33mwget https://cloud-images.ubuntu.com/jammy/current/jammy-server-cloudimg-amd64.img
[0m[33m
[0m[33msudo mv -i jammy-server-cloudimg-amd64.img \
[0m[33m  /var/lib/libvirt/images/templates/ubuntu-22-server.qcow2
[0m[33m  
[0m[33m  
[0m[33msudo apt update \
[0m[33m  && sudo apt install cloud-utils whois -y
[0m[33m  
[0m[33mVM_NAME="UbuS22-nextCloud-1"
[0m[33mUSERNAME="ubuntu"
[0m[33mPASSWORD="Password"
[0m[33m
[0m[33msudo mkdir /var/lib/libvirt/images/$VM_NAME \
[0m[33m  && sudo qemu-img convert \
[0m[33m  -f qcow2 \
[0m[33m  -O qcow2 \
[0m[33m  /var/lib/libvirt/images/templates/ubuntu-22-server.qcow2 \
[0m[33m  /var/lib/libvirt/images/$VM_NAME/root-disk.qcow2
[0m[33m  
[0m[33msudo qemu-img resize \
[0m[33m  /var/lib/libvirt/images/$VM_NAME/root-disk.qcow2 \
[0m[33m  100G
[0m[33m  
[0m[33m  
[0m[33msudo echo "#cloud-config
[0m[33msystem_info:
[0m[33m  default_user:
[0m[33m    name: $USERNAME
[0m[33m    home: /home/$USERNAME
[0m[33m
[0m[33mpassword: $PASSWORD
[0m[33mchpasswd: { expire: False }
[0m[33mhostname: $VM_NAME
[0m[33m
[0m[33m# configure sshd to allow users logging in using password 
[0m[33m# rather than just keys
[0m[33mssh_pwauth: True
[0m[33m" | sudo tee /var/lib/libvirt/images/$VM_NAME/cloud-init.cfg
[0m[33m
[0m[33m
[0m[33msudo cloud-localds \
[0m[33m  /var/lib/libvirt/images/$VM_NAME/cloud-init.iso \
[0m[33m  /var/lib/libvirt/images/$VM_NAME/cloud-init.cfg
[0m[33m  
[0m[33m  
[0m[33msudo virt-install \
[0m[33m  --name $VM_NAME \
[0m[33m  --memory 8192 \
[0m[33m  --disk /var/lib/libvirt/images/$VM_NAME/root-disk.qcow2,device=disk,bus=virtio \
[0m[33m  --disk /var/lib/libvirt/images/$VM_NAME/cloud-init.iso,device=cdrom \
[0m[33m  --os-type linux \
[0m[33m  --os-variant ubuntu16.04 \
[0m[33m  --virt-type kvm \
[0m[33m  --graphics none \
[0m[33m  --network network=my-net,model=virtio \
[0m[33m  --import
[0m[33m  
[0m[33m# Cleanup
[0m[33m
[0m[33msudo rm /var/lib/libvirt/images/$VM_NAME/cloud-init.iso \
[0m[33m  && sudo rm /var/lib/libvirt/images/$VM_NAME/cloud-init.cfg
[0m[33m  
[0m[33msudo virsh edit $VM_NAME
[0m[33m
[0m[33m
[0m[33m# Then remove this section:
[0m[33m<disk type='file' device='cdrom'>
[0m[33m  <driver name='qemu' type='raw'/>
[0m[33m  <source file='/var/lib/libvirt/images/template-ubuntu-22/cloud-init.iso'/>
[0m[33m  <target dev='sda' bus='sata'/>
[0m[33m  <readonly/>
[0m[33m  <address type='drive' controller='0' bus='0' target='0' unit='0'/>
[0m[33m</disk>
[0m[33m
[0m[33m# A Note About Cloning
[0m[33m# Delete match and mac address lines
[0m[33mvim /etc/netplan/50-cloud-init.yaml
[0m[33mnetwork:
[0m[33m    ethernets:
[0m[33m        enp1s0:
[0m[33m            dhcp4: true
[0m[33m            match:
[0m[33m                macaddress: 52:54:00:c2:9e:bc
[0m[33m            set-name: enp1s0
[0m[33m    version: 2
[0m[33m    
[0m[33m# add lines
[0m[33mnetwork:
[0m[33m    ethernets:
[0m[33m        enp1s0:
[0m[33m            dhcp4: true
[0m[33m    version: 2
[0m[33m
[0m[32mâ”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€[0m

]1337;SetMark\[1m[34mâ”„â”„â”„[0m[1m[34mStarting, stopping, and removing KVM instances[0m

[32mâ”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€[0m
